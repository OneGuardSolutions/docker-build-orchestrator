{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "deployer/phar-update": "^2.1",
        "symfony/console": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^7.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    }
}
